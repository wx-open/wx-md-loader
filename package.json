{
  "name": "wx-md-loader",
  "version": "1.0.6",
  "description": "a simple loader parse markdown fence block to JS executable code",
  "keywords": [
    "webpack loader",
    "markdown",
    "javascript"
  ],
  "main": "lib/cjs.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "init:hook": "husky add .husky/commit-msg & husky add .husky/pre-commit",
    "test": "jest --ci --coverage",
    "codecov": "codecov",
    "compile": "gulp",
    "watch": "gulp watchLd",
    "lint": "eslint --ext .tsx,.ts --fix ./src",
    "lint:fix": "eslint --format ./node_modules/eslint-friendly-formatter ./src .eslintrc.js --fix",
    "`prepublishOnly": "npm run compile",
    "pub": "npm publish --access=public"
  },
  "author": "ibreeze2017",
  "license": "MIT",
  "homepage": "https://github.com/wx-open/wx-md-loader",
  "files": [
    "lib",
    "package.json",
    "README.MD"
  ],
  "browserslist": [
    "last 2 version",
    ">1%",
    "ios 7"
  ],
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "cross-env commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-open/wx-md-loader.git"
  },
  "bugs": {
    "url": "https://github.com/wx-open/wx-md-loader/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "babel-loader": "^8.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13"
  },
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@babel/generator": "^7.13.9",
    "@babel/traverse": "^7.13.13",
    "@types/loader-utils": "^2.0.1",
    "@types/markdown-it": "^12.0.1",
    "@types/merge2": "^1.3.0",
    "@types/node": "^11.15.16",
    "@types/prettier": "^2.2.3",
    "@types/through2": "^2.0.36",
    "glob": "^7.1.6",
    "loader-utils": "^2.0.0",
    "markdown-it": "^12.0.4",
    "omit.js": "^2.0.2",
    "prettier": "^2.2.1",
    "schema-utils": "^2.7.0",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.6",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "codecov": "^3.8.1",
    "commitlint": "^12.1.1",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "cross-env": "^7.0.3",
    "merge2": "^1.4.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-typescript": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-rimraf": "^1.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "ts-jest": "^26.5.4",
    "typescript": "^3.3.3"
  }
}
